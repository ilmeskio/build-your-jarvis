# We copy this file to config/.env when we want to override bootstrap defaults locally.
# Leave variables unset to fall back to the script logic, especially inside ephemeral Codespaces.
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_IMAGE=docker.n8n.io/n8nio/n8n:latest
N8N_CONTAINER_NAME=n8n-codespace
N8N_DATA_VOLUME=n8n_data
N8N_TIMEZONE=UTC
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_RUNNERS_ENABLED=true
N8N_PROXY_HOPS=1
# Uncomment the following line if you need to force a custom Codespace hostname (rare).
# N8N_HOST=my-custom-host.example.com
